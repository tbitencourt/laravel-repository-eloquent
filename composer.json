{
    "name"       : "tbitencourt/laravel-repository-eloquent",
    "description": "Easy MVC repository with Eloquent for Laravel 5",
    "version"    : "1.1.1",
    "keywords"   : [
        "repository",
        "eloquent",
        "laravel",
        "php"
    ],
    "license"    : "MIT",
    "homepage"   : "https://github.com/tbitencourt/laravel-repository-eloquent",
    "type"       : "library",
    "authors"    : [
        {
            "name" : "Thales Bitencourt",
            "email": "thalesbitencourt@gmail.com",
            "role" : "Developer"
        }
    ],
    "require"    : {
        "php"              : ">=7.0.0",
        "laravel/framework": "~5.2.0||~5.3.0||~5.4.0||~5.5.0||~5.6.0||~5.7.0||~5.8.0||^6.0.0"
    },
    "autoload"         : {
        "classmap": [
        ],
        "psr-4"   : {
            "App\\"                                   : "app/",
            "Tbitencourt\\LaravelRepositoryEloquent\\": "src"
        }
    },
    "extra"            : {
        "laravel": {
            "providers": [
                "Tbitencourt\\LaravelRepositoryEloquent\\Providers\\RepositoryEloquentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true
}
